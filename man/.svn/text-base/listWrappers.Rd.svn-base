\name{listWrappers}
\alias{listWrappers}

\title{
list all wrapper functions in SuperLearner
}
\description{
List all wrapper functions in \code{\link{SuperLearner}} package
}
\usage{
listWrappers(what = "both")
}

\arguments{
  \item{what}{
What list to return. Can be \code{both} for both prediction algorithms and screening algorithms, \code{SL} for the prediction algorithms, or \code{screen} for the screening algorithms, or anything else will return a list of all functions in the \code{SuperLearner} package
}
}

\value{
Invisible character vector with all exported functions in the SuperLearner package
}

\author{ Eric C Polley \email{eric.polley@nih.gov} }

\seealso{
\code{\link{SuperLearner}}
}
\examples{
listWrappers(what = "SL")
listWrappers(what = "screen")
}
\keyword{utilities}

